    "money",
    "finance",
    "react",
    "nodejs",
    "mongo"
  ],
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "mongoose": "^6.1.1",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.10",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "eslint": "^9.28.0",
    "jest": "^29.7.0",
    "supertest": "^7.1.1"
  },
