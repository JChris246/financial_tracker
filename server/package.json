{
  "name": "finance_app_server",
  "version": "0.2.0",
  "description": "App to track and manage your finances",
  "main": "index.js",
  "scripts": {
    "start": "node financial_tracker_server.js",
    "dev": "nodemon financial_tracker_server.js",
    "test:unit": "NODE_ENV=test jest --runTestsByPath test/**",
    "test:integration": "NODE_ENV=test DB_TYPE=json DB_PATH=test_db RUN_SERVER=false jest --runTestsByPath test_integration/**",
    "test:server": "NODE_ENV=test DB_PATH=test_db DB_TYPE=json node financial_tracker_server.js"
